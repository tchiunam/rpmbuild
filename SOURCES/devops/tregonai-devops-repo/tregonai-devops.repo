name=DevOps Development Yum Repository for Centos OS_VERSION
baseurl=http://devdoyum.hk.tregonai.com/yum-repos/centos/OS_VERSION/devops/$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-DevOps
priority=2
